{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "SENTRY_DISABLE_AUTO_UPLOAD": "true"
      },
      "prebuildCommand": "node scripts/update-version.js",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_CONVEX_URL": "https://valuable-jackal-755.convex.cloud",
        "EXPO_PUBLIC_CONVEX_SITE_URL": "https://valuable-jackal-755.convex.site",
        "SENTRY_DISABLE_AUTO_UPLOAD": "true"
      },
      "prebuildCommand": "node scripts/update-version.js",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "metzgr@gmail.com",
        "ascAppId": "10181672132",
        "appleTeamId": "D45KX59CR8"
      },
      "android": {
        "track": "internal"
      }
    }
  }
}

