{
	"expo": {
		"scheme": "rackd",
		"userInterfaceStyle": "automatic",
		"orientation": "default",
		"web": {
			"bundler": "metro"
		},
		"name": "rackd",
		"slug": "rackd",
		"splash": {
			"image": "assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#000000"
		},
		"plugins": [
			"expo-font",
			[
				"expo-image-picker",
				{
					"photosPermission": "The app accesses your photos to let you upload tournament flyers."
				}
			]
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		}
	}
}
