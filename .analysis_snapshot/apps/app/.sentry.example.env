# Sentry Configuration for Web App
# Copy these variables to your .env file or hosting platform

# Required: Your Sentry DSN
# Get this from: https://sentry.io/settings/[org]/projects/[project]/keys/
VITE_SENTRY_DSN=https://your-dsn@sentry.io/project-id

# Optional: Release version for tracking deployments
# Example: VITE_SENTRY_RELEASE=1.0.0
VITE_SENTRY_RELEASE=

# Optional: Sample rate for performance monitoring (0.0 to 1.0)
# Production: 0.1 (10%), Development: 1.0 (100%)
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1

# Optional: Enable Sentry in development (set to "true" to enable)
VITE_SENTRY_ENABLE_DEV=false




